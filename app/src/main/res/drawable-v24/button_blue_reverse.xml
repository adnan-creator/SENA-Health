<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#DCDCDC" />
            <stroke
                android:width="2dp"
                android:color="#2E4997"/>
            <corners
                android:radius="5dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="@color/white" />
            <stroke
                android:width="2dp"
                android:color="@color/senadark"/>
            <corners
                android:radius="5dp" />
        </shape>
    </item>
</selector>